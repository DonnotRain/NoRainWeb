<div id="dlgMain" style="width: 500px; height: 360px; display: none; padding-top: 5px">
    <form id="fm" style="margin: 0px auto;">
        @{
            var width = 220;
            var easyWidth = 222;
        }
        <table border="0" cellpadding="0" cellspacing="0" class="frm-table">
            <tr>
                <th>
                    类型<span class="label-necessary">*</span>：
                </th>
                <td>
                    <span class="item">
                        <input type="radio" id="IsMenu" name="FunctionType" value="1" style="vertical-align: middle; margin: auto" />
                        <label for="IsMenu" style="vertical-align: middle;">菜单项</label>
                        &nbsp;&nbsp;
                    </span>
                    <span class="item">
                        <input type="radio" id="IsModule" name="FunctionType" value="2" style="vertical-align: middle; margin: auto" />
                        <label for="IsModule" style="vertical-align: middle;">系统模块</label>
                        &nbsp;&nbsp;
                    </span>
                    <span class="item">
                        <input type="radio" id="IsFunction" name="FunctionType" value="3" style="vertical-align: middle; margin: auto;" />
                        <label for="IsFunction" style="vertical-align: middle;">页面功能</label>
                        &nbsp;&nbsp;
                    </span>
                </td>
            </tr>
            <tr>
                <th>
                    模块名称<span class="label-necessary">*</span>：
                </th>
                <td>
                    <input name="Name" class="required" type="text" id="Name" style="width: 90%" value="" />
                </td>
            </tr>
            <tr>
                <th>
                    模块编号<span class="label-necessary">*</span>：
                </th>
                <td>
                    <input name="ControlID" class="required" type="text" style="width: 90%" id="ControlID" value="" />
                </td>
            </tr>
            <tr>
                <th>
                    上级模块<span class="label-necessary">*</span>：
                </th>
                <td>
                    @*<input name="PID" type="hidden" id="PID" value="" />*@
                    <input name="PID" class="required" type="text" id="PID" style="width: 344px;" value="" />
                </td>
            </tr>
            <tr>
                <th>
                    菜单图标：
                </th>
                <td>
                    <i id="iconSample" class="iconmoon-desktop" style="font-size: 20px; vertical-align: middle"></i>
                    图标类名
                    <input id="ImageIndex" name="ImageIndex" value="iconmoon-desktop" style="width: 30%" />
                    <input value="选择图标" class="btn-primary" onclick="chooseIcon();" type="button" />
                </td>
            </tr>
            <tr>
                <th>
                    Web连接地址：
                </th>
                <td>
                    <input name="Path" type="text" id="Path" style="width: 90%" value="" />
                </td>
            </tr>
            @*            <tr>
                    <th>选项<span class="label-necessary">*</span>：
                    </th>
                    <td>
                        <span class="item">
                            <input type="checkbox" id="IsEnabledCheck"  checked="checked" style="vertical-align: middle; margin: auto" />
                            <label for="IsEnabledCheck" style="vertical-align: middle;">有效</label>
                            &nbsp;&nbsp;
                        </span>
                        <span class="item">
                            <input type="checkbox" id="AllowEditCheck" checked="checked" style="vertical-align: middle; margin: auto" />
                            <label for="AllowEditCheck" style="vertical-align: middle;">允许编辑</label>
                            &nbsp;&nbsp;
                        </span>
                        <span class="item">
                            <input type="checkbox" id="AllowDeleteCheck" checked="checked" style="vertical-align: middle; margin: auto;" />
                            <label for="AllowDeleteCheck" style="vertical-align: middle;">允许删除</label>
                            &nbsp;&nbsp;
                        </span>
                    </td>
                </tr>*@
            <tr>
                <th>
                    显示顺序<span class="label-necessary">*</span>：
                </th>
                <td>
                    <input name="Sort" type="text" id="Sort" class="easyui-numberbox required" style="width: 90%" value="" />
                </td>
            </tr>
            <tr>
                <th>
                    授权角色：
                </th>
                <td>
                    <input name="RoleIds" type="text" id="RoleIds" style="width: 344px" />
                </td>
            </tr>
        </table>

        <input type="hidden" id="ID" name="ID" value="" />
    </form>
</div>


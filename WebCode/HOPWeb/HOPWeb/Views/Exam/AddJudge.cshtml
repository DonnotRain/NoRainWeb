<div id="dlgJudge" style="width: 500px; height: 300px; display: none; padding-top: 5px">
    <form id="fmJudge" style="margin: 0px auto;">
        @{
            var width = 220;
            var easyWidth = 222;
        }
        <table border="0" cellpadding="0" cellspacing="0" class="frm-table">
            <tr>
                <th>
                    题目<span class="label-necessary">*</span>：
                </th>
                <td>
                    <input name="Title" id="Title" class="required" type="text" style="width: 90%" value="" />
                </td>
            </tr>
            <tr>
                <th>
                    答案<span class="label-necessary">*</span>：
                </th>
                <td>
                    <select name="Answer" id="Answer" class="easyui-combobox required" style="width: 344px">
                         <option value="1">正确</option>
                         <option value="0">错误</option>
                    </select>
                    
                </td>
            </tr>
            <tr>
                <th>
                    分数<span class="label-necessary">*</span>：
                </th>
                <td>
                    <input name="Score" id="JudgeScore" class="required" type="text" style="width: 90%" value="" />
                </td>
            </tr>
            <tr>
                <th>
                    计时<span class="label-necessary">*</span>：
                </th>
                <td>
                    <input name="AnswerTime" id="AnswerTime" class="required" type="text" style="width: 90%" value="" />
                </td>
            </tr>
            <tr>
                <th>
                   顺序<span class="label-necessary">*</span>：
                </th>
                <td>
                    <input name="Seq" id="Seq" class="required" type="text" style="width: 90%" value="" />
                </td>
            </tr>
        </table>
        <input type="hidden" id="JudgeID" name="ID" value="" />
        <input type="hidden" id="CorpCode" name="CorpCode" value="" />
        <input type="hidden" id="FileId" name="FileId" value="" />
    </form>
</div>
